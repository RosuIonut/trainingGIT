module CC_IPC
{
   //Configuration of APIM base component 
   //that receives DPOOL data change 
   //events and fires syncable event to 
   //the actual dispatcher task 
   component HMITestTools
   {
      
      priority SYSCT_nThreadPrio_HMITestTools;  // set definition in *.sdi system configuration
      autorun;								    // run application
      initial call;							    // boInitialCall() for individual initialization stuff
      
      consumes
      {
         SI_IPC_Timeout; /* Receive Timer/TimeOut events, but all signal ids must be ..         */
                         /* .. taken from the generated file .\adapt\gen\proseco\ipc_idgen.h.   */
         default;        // to handle events by switch case instead callback
      } // consumes
   } // component HMITestTools
}; // module CC_IPC



